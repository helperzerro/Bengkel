<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.uas.bengkel">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="com.uas.bengkel.activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.uas.bengkel.activity.DaftarBengkelActivity" />
        <activity android:name="com.uas.bengkel.activity.PenyewaActivity" />
        <activity android:name="com.uas.bengkel.activity.SewaBengkelActivity" />
        <activity android:name="com.uas.bengkel.activity.DetailBengkelActivity" />
        <activity android:name="com.uas.bengkel.activity.DetailPenyewaActivity" />
        <activity android:name="com.uas.bengkel.activity.RegisterActivity"/>
        <activity android:name="com.uas.bengkel.activity.MainActivity"/>
    </application>

</manifest>